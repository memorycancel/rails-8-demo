<p><%= notice %></p>

<% content_for :title, "Posts" %>

<h1><%= t :posts %></h1>

<div id="posts">
  <ul>
    <% @posts.each do |post| %>
      <% cache post do %>
        <li><%= link_to post.title, post_path(post) %></li>
      <% end %>
    <% end %>
  </ul>
</div>


<%= link_to "New post", new_post_path %>
